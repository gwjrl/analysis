    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "x = np.linspace(-2,2,1500)\n",
    "y1 = np.sqrt(1 - (abs(x) - 1)**2)\n",
    "y2 = -3*np.sqrt( 1 - (abs(x)/2)**0.5)\n",
